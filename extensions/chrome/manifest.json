{
  "version": "1.0.0",
  "name": "React Components Form Debugger",
  "short_name": "React Components Form Debugger",
  "description": "Extension for debugging react components forms",
  "minimum_chrome_version": "10.0",
  "manifest_version": 2,
  "devtools_page": "./devtools.html",
  "background": {
    "scripts": ["./background.js"],
    "persistent": false
  },
  "permissions": [ "notifications", "contextMenus", "tabs", "storage", "<all_urls>", "activeTab" ]
}